ivider />\r\n          {toggle === 'machine learning' ?\r\n            <ToggleButton active value=\"machine learning\" onClick={() => setToggle('machine learning')}>MACHINE LEARNING</ToggleButton>\r\n            :\r\n            <ToggleButton value=\"machine learning\" onClick={() => setToggle('machine learning')}>MACHINE LEARNING</ToggleButton>\r\n          }\r\n        </ToggleButtonGroup>\r\n        <CardContainer>\r\n          {toggle === 'all' && projects\r\n            .map((project) => (\r\n              <ProjectCard project={project} openModal={openModal} setOpenModal={setOpenModal}/>\r\n            ))}\r\n          {projects\r\n            .filter((item) => item.category == toggle)\r\n            .map((project) => (\r\n              <ProjectCard project={project} openModal={openModal} setOpenModal={setOpenModal}/>\r\n            ))}\r\n        </CardContainer>\r\n      </Wrapper>\r\n    </Container>\r\n  )\r\n}\r\n\r\nexport default Projects",[],"C:\\Users\\USER\\WebstormProjects\\Portfolio-React-New\\src\\components\\Contact\\index.js",[],[],"C:\\Users\\USER\\WebstormProjects\\Portfolio-React-New\\src\\components\\Footer\\index.js",["121"],[],"import styled from 'styled-components';\r\nimport FacebookIcon from '@mui/icons-material/Facebook';\r\nimport TwitterIcon from '@mui/icons-material/Twitter';\r\nimport LinkedInIcon from '@mui/icons-material/LinkedIn';\r\nimport InstagramIcon from '@mui/icons-material/Instagram';\r\nimport { Bio } from '../../data/constants';\r\n\r\nconst FooterContainer = styled.div`\r\n  width: 100%;\r\n  padding: 2rem 0;\r\n  display: flex;\r\n  justify-content: center;\r\n  //background: linear-gradient(100.26deg, rgba(0, 102, 255, 0.05) 42.33%, rgba(150, 0, 225, 0.05) 127.07%);\r\n`;\r\n\r\n\r\nconst FooterWrapper = styled.footer`\r\n  width: 100%;\r\n  max-width: 1200px;\r\n  display: flex;\r\n  flex-direction: column;\r\n  gap: 14px;\r\n  align-items: center;\r\n  padding: 1rem;\r\n  color: ${({ theme }) => theme.text_primary};